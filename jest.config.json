{
  "preset": "react-native",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "setupFiles": [
    "<rootDir>/jest/setup.ts"
  ],
  "setupFilesAfterEnv": [
    "<rootDir>/jest/setup.env.ts"
  ],
  "transformIgnorePatterns": [
    "node_modules/(?!(jest-)?@?react-native|@react-native-community|@react-navigation)"
  ]
}